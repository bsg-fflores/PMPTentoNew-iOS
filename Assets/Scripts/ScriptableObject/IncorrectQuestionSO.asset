        tieneRetroalimentacion: 1
        urlRetroalimentacionVideo: https://players.brightcove.net/6267108632001/rEr9tuuTvS_default/index.html?videoId=6318477001112
        retroalimentacion: "Est\xE1 en el nivel 4 de la pir\xE1mide de Maslow. Cubiertas
